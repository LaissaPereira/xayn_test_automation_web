{
    "compilerOptions": {
      "baseUrl": ".",
      "target": "es2017",
      "lib": ["es2017", "dom"],
      "types": ["cypress","node"],
      "module": "commonjs",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "strict": true,
      "esModuleInterop": true,
      "rootDir": ".",
      "outDir": "dist"              
    },
    "include": [
      "cypress/**/*.ts",
      ".github/scripts/**/*.ts"
    ]
  }
  